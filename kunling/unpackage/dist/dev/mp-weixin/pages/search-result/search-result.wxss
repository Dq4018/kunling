
.search-result-container.data-v-66beb00c {
	min-height: 100vh;
	background-color: #f5f5f5;
}

/* 顶部导航栏 */
.top-nav.data-v-66beb00c {
	height: 88rpx;
	padding: 0 32rpx;
	background-color: #fff;
	display: flex;
	align-items: center;
	justify-content: space-between;
