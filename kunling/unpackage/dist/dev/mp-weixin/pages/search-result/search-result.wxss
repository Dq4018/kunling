}

/* 搜索信息 */
.search-info.data-v-66beb00c {
	margin: 108rpx 32rpx 24rpx;
	padding: 24rpx;
	background-color: #fff;
	border-radius: 12rpx;
	display: flex;
	flex-wrap: wrap;
	align-items: center;
	gap: 16rpx;
}
.search-keyword.data-v-66beb00c {
	font-size: 28rpx;
